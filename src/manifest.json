{
  "name": "ProjectHuddle Sync",
  "description": "Sync a Project with your ProjectHudle feedback plugin.",
  "author": "ProjectHuddle",
  "homepage": "https://projecthuddle.io",
  "version": "0.0.6",
  "compatibleVersion": "55",
  "$schema": "https://raw.githubusercontent.com/BohemianCoding/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "icon.png",
  "commands": [
    {
      "name": "Sync...",
      "identifier": "projecthuddle.sync",
      "shortcut": "cmd p s",
      "script": "./sync.js"
    },
    {
      "name": "View Project",
      "identifier": "projecthuddle.view",
      "script": "./view.js"
    },
    {
      "name": "Connection Settings...",
      "identifier": "projecthuddle.connection",
      "script": "./connection.js"
    }
  ],
  "menu": {
    "title": "ProjectHuddle",
    "items": [
      "projecthuddle.sync",
      "projecthuddle.view",
      "projecthuddle.connection"
    ]
  }
}
